<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ethnicindia.omkarmic.kalyanam">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MyRegistrations"></activity>
        <activity android:name=".miscellaneous.MyProfile" />
        <activity android:name=".miscellaneous.ContactUs" />
        <activity android:name=".miscellaneous.AboutUs" />
        <activity android:name=".PostRegistration" />
        <activity android:name=".EventRegistration" />
        <activity android:name=".EventDetails" />
        <activity android:name=".authentication.SignUp" />
        <activity android:name=".authentication.Login" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>